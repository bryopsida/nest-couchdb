{
  "name": "@bryopsida/nest-couchdb",
  "description": "CouchDB module for Nest framework",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepack": "npm run build",
    "start:services": "docker-compose up -d",
    "stop:services": "docker-compose down",
    "build": "node_modules/typescript/bin/tsc --project ./",
    "build:docs": "npx @compodoc/compodoc -p tsconfig.json  -d ./docs",
    "test": "jest --coverage --testPathPattern='test/.*\\.spec\\.ts'",
    "lint": "eslint --ext .ts src/ && prettier --check .",
    "lint:fix": "eslint --ext .ts src/ --fix && prettier --write ."
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@jest/globals": "^29.5.0",
    "@nestjs/microservices": "9.x.x",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/testing": "9.x.x",
    "@types/jest": "^29.5.2",
    "@types/node": "18.x.x",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "jest": "29.x.x",
    "prettier": "^2.8.8",
    "testcontainers": "^9.8.0",
    "ts-jest": "29.x.x",
    "typescript": "4.x.x"
  },
  "dependencies": {
    "@nestjs/common": "9.4.2",
    "@nestjs/core": "9.4.2",
    "nano": "^10.1.2",
    "pouchdb": "^8.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testTimeout": 240000,
    "verbose": true,
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "transform": {
      "\\.[jt]sx?$": [
        "ts-jest"
      ]
    }
  }
}
